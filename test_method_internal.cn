// 测试方法内部的字符串拼接
using lib <io>;
using ns std;

class TestClass {
    private secret : int;
    public visible : int;
    
    constructor(value : int) {
        this.secret = value;
        this.visible = value * 3;
    };
    
    public fn testInternalAccess() : void {
        std::println("内部访问secret: " + this.secret);
        std::println("内部访问visible: " + this.visible);
    };
    
    public fn getSecret() : int {
        return this.secret;
    };
};

fn main() : int {
    std::println("测试方法内部字符串拼接");
    
    obj : TestClass = new TestClass(42);
    
    std::println("调用testInternalAccess方法:");
    obj.testInternalAccess();
    
    std::println("调用getSecret方法: " + obj.getSecret());
    
    return 0;
};
