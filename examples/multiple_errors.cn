using lib_once <io>;

// 错误1: 缺少分号
let global_var : int = 10

// 错误2: 未定义的函数
fn undefined_function() : int {
    // 错误3: 未定义的变量
    print(undefined_var);
    
    // 错误4: 类型不匹配
    let s : string = 42;
    
    // 错误5: 语法错误，缺少右括号
    print("Hello, World!";
    
    // 错误6: 缺少分号
    let x : int = 5
    
    // 错误7: 函数调用参数错误
