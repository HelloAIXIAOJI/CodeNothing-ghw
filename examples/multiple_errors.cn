    return 0;
};

fn main() : int {
    // 错误8: 错误的函数调用格式
    print "Direct call without parentheses";
    
    // 错误9: 类型不匹配的赋值
    let num : int = "string";
    
    // 错误10: 未定义变量的使用
    let result : int = x + y;
    
    return 0;
}; 