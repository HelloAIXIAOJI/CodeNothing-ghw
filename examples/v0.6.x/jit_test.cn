// 🚀 JIT编译器测试程序
using lib <io>;
using ns std;

fn main() : int {
    std::println("🚀 JIT编译器测试开始");
    
    // 测试简单的算术运算
    a : int = 10;
    b : int = 20;
    c : int = a + b;
    
    std::println("a = " + a);
    std::println("b = " + b);
    std::println("c = a + b = " + c);
    
    // 测试循环
    i : int = 0;
    sum : int = 0;
    while (i < 5) {
        sum = sum + i;
        std::println("i = " + i + ", sum = " + sum);
        i = i + 1;
    };
    
    std::println("最终sum = " + sum);
    std::println("🎉 JIT编译器测试完成！");
    
    return 0;
};
