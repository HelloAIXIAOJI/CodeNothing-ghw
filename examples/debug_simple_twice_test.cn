using lib <io>;
using ns std;

// 调试简单的两次调用测试

fn simpleAdd(a : int, b : int) : int {
    return a + b;
};

fn main() : int {
    std::println("=== 调试简单的两次调用测试 ===");
    
    // 通过函数指针调用两次
    funcPtr : *fn(int, int) : int = simpleAdd;
    
    std::println("第一次调用:");
    result1 : int = funcPtr(5, 3);
    std::println("funcPtr(5, 3) = " + result1);
    
    std::println("第二次调用:");
    result2 : int = funcPtr(2, 7);
    std::println("funcPtr(2, 7) = " + result2);
    
    std::println("=== 测试完成 ===");
    return 0;
};
