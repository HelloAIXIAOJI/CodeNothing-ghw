using lib <io>;
using ns std;

// CodeNothing 类型错误测试

fn main() : int {
    std::println("=== CodeNothing 类型错误测试 ===");
    
    // 类型不匹配错误
    x : int = "这应该是一个字符串，但声明为int";
    
    // 函数参数类型错误
    result : int = add("hello", 42);  // 第一个参数应该是int，但传入了string
    
    // 返回类型错误
    return "应该返回int，但返回了string";
};

fn add(a : int, b : int) : int {
    return a + b;
};

fn wrongReturn() : int {
    return "这应该返回int，但返回了string";
};
