using lib <io>;
using ns std;

// 简单Lambda测试

fn main() : int {
    std::println("=== 简单Lambda测试 ===");
    
    // 测试最简单的Lambda表达式
    increment : *fn(int) : int = (x => x + 1);
    result : int = increment(5);
    std::println("increment(5) = " + result);
    
    std::println("=== 测试完成 ===");
    return 0;
};
