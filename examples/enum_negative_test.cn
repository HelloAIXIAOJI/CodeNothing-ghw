using lib <io>;
using ns std;

// 测试负数参数的枚举

enum TestEnum {
    PositiveFloat(float),
    NegativeFloat(float),
    ZeroFloat(float)
};

fn main() : int {
    std::println("=== 负数枚举测试 ===");
    
    // 测试正数
    positive : TestEnum = TestEnum::PositiveFloat(42.5);
    std::println("正数: " + positive);
    
    // 测试零
    zero : TestEnum = TestEnum::ZeroFloat(0.0);
    std::println("零: " + zero);
    

    
    return 0;
};
