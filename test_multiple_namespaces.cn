fn main() : int {
    // 导入多个命名空间
    using ns math;
    using ns utils;
    
    // 使用导入的命名空间中的函数
    result1 : int = add(10, 5);           // 调用math::add，结果是15
    result2 : int = format(result1);      // 调用utils::format，结果是115 (字符串长度)
    
    // 使用全局函数
    result3 : int = ::add(10, 5);         // 调用全局add，结果是20
    
    // 使用命名空间路径
    result4 : int = math::math2::add(10, 5);  // 调用math::math2::add，结果是50
    result5 : int = utils::helper::process(10);  // 调用utils::helper::process，结果是30
    
    return result1 + result2 + result3 + result4 + result5;  // 15 + 115 + 20 + 50 + 30 = 230
};

// 全局函数
fn add(a : int, b : int) : int {
    return a * 2;  // 故意与命名空间中的函数实现不同
};

// 数学命名空间
ns math {
    fn add(a : int, b : int) : int {
        return a + b;
    };
    
    ns math2 {
        fn add(a : int, b : int) : int {
            return a * b;
        };
    };
};

// 工具命名空间
ns utils {
    fn format(num : int) : int {
        return 100 + num;  // 简单处理，返回数字+100
    };
    
    ns helper {
        fn process(value : int) : int {
            return value * 3;
        };
    };
}; 