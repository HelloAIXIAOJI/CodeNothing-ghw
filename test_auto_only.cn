// 仅测试Auto类型推断
using lib <io>;
using ns std;

fn main() : int {
    std::println("测试Auto类型推断");
    
    // 测试1: Auto + Auto
    a : Auto = 10;
    b : Auto = 20;
    sum : Auto = a + b;
    std::println("Auto + Auto: " + sum);
    
    // 测试2: Auto + int
    c : Auto = 5;
    d : int = 15;
    mixed : Auto = c + d;
    std::println("Auto + int: " + mixed);
    
    // 测试3: Auto字符串
    greeting : Auto = "Hello ";
    name : Auto = "World!";
    message : Auto = greeting + name;
    std::println("Auto字符串: " + message);
    
    std::println("Auto类型推断测试完成！");
    return 0;
};
