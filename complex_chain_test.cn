using lib <io>;

fn main() : int {
    str : string = "  Hello World  ";
    std::println("原始字符串: '" + str + "'");
    
    // 链式调用：trim -> to_upper -> substring
    result : string = str.trim().to_upper().substring(0, 5);
    std::println("链式调用结果: '" + result + "'");
    
    // 测试字符串长度
    length : int = str.trim().length();
    std::println("字符串长度: " + length);
    
    return 0;
}; 