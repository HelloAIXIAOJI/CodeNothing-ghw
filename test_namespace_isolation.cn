using lib <io>;

fn main() : int {
    println("=== 测试namespace隔离 ===");
    
    // 这个调用应该失败，因为没有导入ns1命名空间
    func1("这应该失败");
    
    return 0;
};

ns ns1 {
    fn func1(msg : string) : void {
        println("ns1::func1: " + msg);
    };
};

ns ns2 {
    fn func2(msg : string) : void {
        println("ns2::func2: " + msg);
    };
};
