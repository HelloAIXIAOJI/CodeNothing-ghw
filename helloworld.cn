fn main() : int {
    return dot();
};

fn dot() : int {
    num : int = Y + 1;             // 变量定义与初始化，需指定类型
    num--;                         // 自减操作
    num-= 3;                       // 复合赋值操作
    num++;                         // 自增操作
    num+= 3;                       // 复合赋值操作
    num*= 3;                       // 复合赋值操作
    num/= 3;                       // 复合赋值操作
    num%= 3;                       // 复合赋值操作
    num = num + 1;                   // 赋值操作
    return 0;
};