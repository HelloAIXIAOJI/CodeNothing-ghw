fn main() : int {
    return dot();
};

fn dot() : int {
    num : int = 10;             // 变量定义与初始化，需指定类型
    num++;                      // 自增操作
    num--;                      // 自减操作
    num += 3;                   // 加法复合赋值操作
    num -= 2;                   // 减法复合赋值操作
    num *= 4;                   // 乘法复合赋值操作
    num /= 2;                   // 除法复合赋值操作
    num %= 5;                   // 取模复合赋值操作
    return num;                 // 返回最终结果
};