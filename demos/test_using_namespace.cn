fn main() : int {
    // 导入math命名空间
    using ns math;
    
    // 现在可以直接调用math命名空间中的函数，无需前缀
    result1 : int = add(10, 5);  // 直接调用math::add，结果应该是15
    
    // 调用全局函数，需要明确指定
    result2 : int = ::add(10, 5);  // 调用全局add，结果应该是20
    
    // 调用命名空间函数，使用完整路径
    result3 : int = math::math2::add(10, 5);  // 结果应该是50
    
    return result1 + result2 + result3;  // 15 + 20 + 50 = 85
};

// 全局函数
fn add(a : int, b : int) : int {
    return a * 2; // 故意与命名空间中的函数实现不同
};

// 命名空间
ns math {
    // 命名空间内的函数
    fn add(a : int, b : int) : int {
        return a + b;
    };
    
    // 嵌套命名空间
    ns math2 {
        fn add(a : int, b : int) : int {
            return a * b;
        };
    };
}; 