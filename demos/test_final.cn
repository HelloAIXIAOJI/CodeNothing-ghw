fn main() : int {
    // 测试各种数据类型
    i : int = 42;
    f : float = 3.14;
    b : bool = true;
    s : string = "Hello, CodeNothing!";
    l : long = 9223372036854775807;  // 最大的长整型值
    
    // 测试数组
    numbers : int[] = [1, 2, 3, 4, 5];
    
    // 测试映射
    scores : Map<string, int> = {
        "Alice": 95,
        "Bob": 87,
        "Charlie": 92
    };
    
    // 测试复杂表达式
    result : int = calculate();
    
    return result;
};

fn calculate() : int {
    a : int = 10;
    b : int = 5;
    c : int = 2;
    
    // 测试复杂表达式和运算符优先级
    result : int = a * b + a / c - b % c;  // 10 * 5 + 10 / 2 - 5 % 2 = 50 + 5 - 1 = 54
    
    return result;
}; 