fn main() : int {
    // 导入命名空间
    using ns math;
    
    // 调用全局函数
    global_result : int = ::add(10, 5);
    
    // 调用导入的命名空间函数
    math_result : int = add(20, 10);
    
    // 调用嵌套命名空间函数
    nested_result : int = math2::add(30, 15);
    
    return global_result + math_result + nested_result;
};

// 全局函数
fn add(a : int, b : int) : int {
    return a * 2;
};

// 命名空间
ns math {
    // 命名空间内的函数
    fn add(a : int, b : int) : int {
        return a + b;
    };
    
    // 嵌套命名空间
    ns math2 {
        fn add(a : int, b : int) : int {
            return a * b;
        };
    };
}; 