fn main() : int {
    // 测试for循环
    sum1 : int = for_loop_test();
    
    // 测试while循环
    sum2 : int = while_loop_test();
    
    // 返回两个测试的总和
    return sum1 + sum2;
};

// 测试for循环
fn for_loop_test() : int {
    sum : int = 0;  // 初始化累加器
    
    // 使用for循环计算1到5的和
    for (i : 1..5) {
        sum += i;   // 累加当前值
    };
    
    return sum;     // 返回计算结果，应为15 (1+2+3+4+5)
};

// 测试while循环
fn while_loop_test() : int {
    sum : int = 0;  // 初始化累加器
    counter : int = 1;  // 初始化计数器
    
    // 使用while循环计算1到5的和
    while (counter <= 5) {
        sum += counter;  // 累加当前值
        counter++;       // 增加计数器
    };
    
    return sum;     // 返回计算结果，应为15 (1+2+3+4+5)
}; 