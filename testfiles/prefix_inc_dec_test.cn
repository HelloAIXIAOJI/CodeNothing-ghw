fn main() : int {
    // 测试前置和后置自增/自减
    return test_inc_dec();
};

fn test_inc_dec() : int {
    // 初始化变量
    a : int = 10;
    b : int = 10;
    c : int = 10;
    d : int = 10;
    
    // 测试后置自增
    a++;        // a变为11
    
    // 测试后置自减
    b--;        // b变为9
    
    // 测试前置自增
    ++c;        // c变为11
    
    // 测试前置自减
    --d;        // d变为9
    
    // 返回所有变量的和，应该是11+9+11+9=40
    return a + b + c + d;
}; 