fn main() : int {
    // 导入多个命名空间，它们有同名函数
    using ns math;
    using ns utils;
    
    // 这里会有歧义，因为math和utils都有process函数
    result : int = process(10);  // 这行会导致编译错误
    
    // 使用完整路径避免歧义
    result1 : int = math::process(10);  // 结果是100
    result2 : int = utils::process(10);  // 结果是30
    
    return result1 + result2;  // 100 + 30 = 130
};

// 数学命名空间
ns math {
    fn process(a : int) : int {
        return a * 10;
    };
};

// 工具命名空间
ns utils {
    fn process(a : int) : int {
        return a * 3;
    };
}; 