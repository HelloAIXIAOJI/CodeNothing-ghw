using lib <io>;
using ns std;
using ns mm;

fn main() : int {
    println("=== 测试修复后的namespace隔离 ===");
    
    // 方式1: 使用导入的命名空间函数
    r("通过导入的命名空间调用");
    
    // 方式2: 使用完整路径调用
    mm::r("通过完整路径调用");
    
    // 方式3: 使用std命名空间函数
    std::println("通过std命名空间调用");
    
    println("namespace隔离修复测试完成!");
    return 0;
};

ns mm {
    fn r(msg : string) : void {
        std::println("mm::r: " + msg);
    };
};
