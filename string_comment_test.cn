fn main() : int {
    // 测试字符串中包含注释符号的情况
    
    // 1. 字符串中包含单行注释符号
    str1 : string = "这是一个字符串，包含//注释符号";
    
    // 2. 字符串中包含多行注释符号
    str2 : string = "这是一个字符串，包含/!多行注释符号!/";
    
    // 3. 字符串中包含转义的引号
    str3 : string = "这是一个字符串，包含\"引号\"";
    
    // 4. 字符串中包含转义的注释符号
    str4 : string = "这是一个字符串，包含\\/\\/转义的注释符号";
    
    // 5. 多行字符串（实际上CodeNothing不支持，但测试换行处理）
    str5 : string = "这是第一行
这是第二行，包含//注释符号
这是第三行";
    
    // 6. 字符串后紧跟注释
    str6 : string = "字符串后紧跟注释"; // 这是注释
    
    // 7. 字符串后紧跟多行注释
    str7 : string = "字符串后紧跟多行注释"; /! 这是多行注释 !/
    
    // 8. 注释中包含字符串
    // 这是注释，包含引号内容
    
    // 9. 多行注释中包含字符串
    /!
        这是多行注释，包含引号内容
        还包含更多引号内容
    !/
    
    // 所有字符串都成功定义，返回成功
    return 0;
}; 