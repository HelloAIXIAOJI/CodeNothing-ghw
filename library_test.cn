
using lib <time>;
using lib <os>;
using lib <io>;

fn main() : int {
    // 测试io库中的println函数
    println("测试直接函数调用 (不需要命名空间前缀)");
    std::println("测试命名空间函数调用 (带std前缀)");
    
    // 测试时间库函数
    println("当前时间戳: " + now());
    println("格式化时间: " + format_now(""));
    
    // 测试系统库函数
    println("操作系统: " + os_name());
    println("用户名: " + username());
    
    return 0;
};

