using lib <io>;

fn main() : int {
    str : string = "  Hello World  ";
    std::println("原始字符串: '" + str + "'");
    
    // 测试单个方法调用
    trimmed : string = str.trim();
    std::println("去除空格: '" + trimmed + "'");
    
    // 测试链式调用
    result : string = str.trim().to_upper();
    std::println("链式调用结果: '" + result + "'");
    
    return 0;
}; 