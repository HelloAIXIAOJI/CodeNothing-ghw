// 仅测试this关键字
using lib <io>;
using ns std;

class TestThis {
    public value : int;
    
    constructor(val : int) {
        this.value = val;
    };
    
    public fn testThis() : void {
        std::println("测试this关键字");
        // 直接输出this
        std::println(this);
    };
};

fn main() : int {
    std::println("测试this关键字");
    
    obj : TestThis = new TestThis(42);
    obj.testThis();
    
    return 0;
};
