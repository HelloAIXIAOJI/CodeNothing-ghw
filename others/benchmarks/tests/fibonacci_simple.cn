using lib <io>;
using ns std;

// CodeNothing 简单斐波那契数列性能测试
// 只测试迭代版本，计算第1000个斐波那契数

fn iterativeFibonacci(n : int) : int {
    if (n <= 1) {
        return n;
    };

    a : int = 0;
    b : int = 1;
    i : int = 2;

    while (i <= n) {
        temp : int = a + b;
        a = b;
        b = temp;
        i = i + 1;
    };

    return b;
};

fn main() : int {
    std::println("=== CodeNothing 简单斐波那契数列性能测试 ===");
    
    // 测试参数
    n : int = 30;

    std::println("计算第 " + n + " 个斐波那契数 (迭代版本)");

    // 迭代版本测试
    result : int = iterativeFibonacci(n);
    std::println("iterativeFibonacci(" + n + ") = " + result);
    
    std::println("✓ 测试完成");
    
    return 0;
};
