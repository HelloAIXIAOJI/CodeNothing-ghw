// 🔍 简化嵌套命名空间测试 - 验证基础功能
using lib <io>;
using ns std;

fn main() : int {
    std::println("🔍 简化嵌套命名空间测试");
    
    // 测试二级嵌套
    std::println("测试二级嵌套:");
    outer::inner::hello();
    
    std::println("测试完成");
    return 0;
};

// 二级嵌套命名空间
ns outer {
    ns inner {
        fn hello() : void {
            std::println("  ✓ 嵌套函数调用成功");
        };
    };
};
