using lib <io>;

fn fib(n : int) : int {
    if (n <= 1) {
        return n;
    } else {
        return fib(n - 1) + fib(n - 2);
    };
};

fn main() : int {
    std::println("请输入要计算的斐波那契项数 n：");
    input_str : string = std::input();
    n : int = input_str;
    result : int = fib(n);
    std::println("fib(" + n + ") = " + result);
    return 0;
};