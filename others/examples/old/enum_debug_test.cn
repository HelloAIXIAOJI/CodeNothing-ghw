using lib <io>;
using ns std;

// 调试枚举解析问题

enum TestEnum {
    IntValue(int),
    FloatValue(float),
    StringValue(string)
};

fn main() : int {
    std::println("=== 枚举调试测试 ===");
    
    // 测试整数参数
    intEnum : TestEnum = TestEnum::IntValue(42);
    std::println("整数枚举: " + intEnum);
    
    // 测试浮点数参数
    floatEnum : TestEnum = TestEnum::FloatValue(3.14);
    std::println("浮点数枚举: " + floatEnum);
    
    // 测试字符串参数
    stringEnum : TestEnum = TestEnum::StringValue("测试");
    std::println("字符串枚举: " + stringEnum);
    
    return 0;
};
