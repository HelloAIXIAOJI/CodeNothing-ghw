// common_utils.cn - 公共工具函数库

using lib <io>;

// 打印分隔线
fn print_separator() : void {
    println("----------------------------------------");
    return;
};

// 打印消息
fn print_message(message: string) : void {
    println("消息: " + message);
    return;
};

// 打印调试信息
fn print_debug(info: string) : void {
    println("[调试] " + info);
    return;
}; 