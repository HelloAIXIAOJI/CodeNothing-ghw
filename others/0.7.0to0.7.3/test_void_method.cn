// 测试void方法调用
using lib <io>;
using ns std;

class TestClass {
    public value : int;
    
    constructor(val : int) {
        this.value = val;
    };
    
    public fn printValue() : void {
        std::println("方法内部输出: " + this.value);
    };
    
    public fn getValue() : int {
        return this.value;
    };
};

fn main() : int {
    std::println("测试void方法调用");
    
    obj : TestClass = new TestClass(42);
    
    std::println("调用printValue方法:");
    obj.printValue();
    
    std::println("调用getValue方法: " + obj.getValue());
    
    return 0;
};
