// 调试this关键字解析
using lib <io>;
using ns std;

class TestThis {
    public value : int;
    
    constructor(val : int) {
        this.value = val;
    };
    
    public fn debugThis() : void {
        std::println("开始调试this");
        // 直接输出this.value
        std::println("this.value的值:");
        std::println(this.value);
    };
};

fn main() : int {
    std::println("调试this关键字解析");
    
    obj : TestThis = new TestThis(42);
    obj.debugThis();
    
    return 0;
};
