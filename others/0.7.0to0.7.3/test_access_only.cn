// 仅测试访问修饰符
using lib <io>;
using ns std;

class TestClass {
    public value : int;
    
    constructor(val : int) {
        this.value = val;
    };
    
    public fn getValue() : int {
        return this.value;
    };
};

fn main() : int {
    std::println("测试访问修饰符");
    
    obj : TestClass = new TestClass(42);
    std::println("对象值: " + obj.value);
    std::println("方法返回: " + obj.getValue());
    
    std::println("访问修饰符测试完成！");
    return 0;
};
